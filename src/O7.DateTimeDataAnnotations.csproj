<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>O7.DateTimeDataAnnotations</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
    <Authors>betobarros07</Authors>
    <Description>DateTime DataAnnotations is a lib that contain a lot of DataAnnotations to validate some DateTime functions, like Greater Than, Less Than, etc.</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>First release for .Net Standard pack</PackageReleaseNotes>
    <Copyright>Copyright 2017 (c)</Copyright>
    <PackageTags>AspNetCore CustomDataAnnotations DateTimeValidations DateTimeFunctions</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.DataAnnotations" Version="2.0.0" />
  </ItemGroup>

</Project>
